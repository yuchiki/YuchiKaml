let makeTuple a b condition = if condition then a else b in
let fst t = t true in
let snd t = t false in
let Nil = "nil" in
let isNil l = l == Nil in

let rec map f l =
    if isNil l then Nil
    else
        let car = fst l in
        let cdr = snd l in
        makeTuple (f car) (map f cdr) in
let rec range minValue maxValue =
    if minValue > maxValue then Nil
    else makeTuple minValue (range (minValue + 1) maxValue) in

let list =  (range 5 10) in
let list = map (\x -> x * x) list in
fst list
